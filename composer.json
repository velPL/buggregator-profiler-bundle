{
    "name": "velpl/buggregator-profiler-bundle",
    "description": "Symfony bundle integrating Buggregator profiler",
    "type": "library",
    "license": "MIT",
    "version": "0.1.0",
    "autoload": {
        "psr-4": {
            "Velpl\\BuggregatorProfilerBundle\\": "src/",
            "Velpl\\BuggregatorProfilerBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Piotr Skowron",
            "email": "poczta@piotrskowron.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "spiral-packages/profiler": "^1.2"
    },
    "extra": {
        "symfony": {
            "require": "6.4.* | ^7.3",
            "allow-contrib": true
        },
        "allow-contrib": true
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "symfony/browser-kit": "6.4.* || ^7.3",
        "symfony/config": "^6 || ^7.3",
        "symfony/css-selector": "6.4.* || ^7.3",
        "symfony/dependency-injection": "^6 || ^7.3",
        "symfony/dotenv": "6.4.* || ^7.3",
        "symfony/flex": "^2.5",
        "symfony/framework-bundle": "6.4.* || ^7.3",
        "symfony/http-kernel": "^6 || ^7.3",
        "symfony/phpunit-bridge": "^6 || ^7.2",
        "symfony/yaml": "6.4.* || ^7.3",
        "symfony/stopwatch": "6.4.* || ^7.3",
        "friendsofphp/php-cs-fixer": "^3.84",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-symfony": "^2.0"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
